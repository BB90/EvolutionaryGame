import networkx as nx
import matplotlib.pyplot as plt
path='ws_test.xls'
#WS = nx.random_graphs.watts_strogatz_graph(1000,5,0.3) #小世界网络
## BA scale-free degree network graphy
BA = nx.barabasi_albert_graph(1000, 4) #无标度网络
pos = nx.circular_layout(BA)          #定义一个布局，此处采用了circular布局方式
print BA.edges()
nx.draw(BA,pos,with_labels=False,node_size = 30)  #绘制图形
plt.show()
nx.write_edgelist(BA, path, delimiter='\t', encoding='utf-8', data=False)
#dicts={1:[2],2:[],3:[6]}
#mydict = {k: v for k, v in dicts.items() if len(v) > 0}
#print mydict


